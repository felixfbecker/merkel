{
  "compilerOptions": {
    "target": "es6",
    "noImplicitAny": true,
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "dist",
    "sourceMap": true,
    "declaration": true,
    "typeRoots": [
      "typings/global",
      "typings/modules"
    ]
  },
  "exclude": [
    "node_modules",
    "dist",
    "migrations"
  ]
}
